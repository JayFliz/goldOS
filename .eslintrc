{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "forOf": true,
    "jsx": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": 0,
    "new-cap": 0,
    "prefer-template": 0,
    "max-len": 0
  }
}
